<%= render 'header', company: @company, headquarters: @headquarters %>
<div>
	<ul>
		<% @members.each do |member| %>
			<li>
				<%= link_to user_path(member) do %>
					<%= attachment_image_tag member, :image, size: '150x150' %>
				<% end %>
			</li>
			<h4><%= member.name %></h4>
		<% end %>
	</ul>
</div>